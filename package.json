{
    "name": "node-slate",
    "version": "1.8.3",
    "description": "Node port of tripit/slate",
    "license": "Apache-2.0",
    "main": "build/index.html",
    "repository": "github:center-key/node-slate",
    "homepage": "https://github.com/center-key/node-slate",
    "bugs": "https://github.com/center-key/node-slate/issues",
    "docs": "https://github.com/center-key/node-slate#readme",
    "keywords": [
        "slate",
        "nodejs",
        "api",
        "api-documentation",
        "static-site-generator"
    ],
    "author": "Various",
    "jshintConfig": {
        "esversion": 6,
        "strict": "implied",
        "node": true,
        "undef": true,
        "unused": true
    },
    "scripts": {
        "test": "jshint gulpfile.js",
        "start": "gulp build-uncompressed && gulp serve",
        "prebuild": "gulp clean",
        "build": "gulp build-static-site",
        "postbuild": "gulp lint",
        "deploy": "./deploy.sh"
    },
    "dependencies": {
        "del": "~3.0",
        "gulp": "~4.0",
        "gulp-clean-css": "~3.10",
        "gulp-cli": "~2.0",
        "gulp-concat": "~2.6",
        "gulp-ejs": "~3.2",
        "gulp-htmlhint": "~2.2",
        "gulp-if": "~2.0",
        "gulp-jshint": "~2.1",
        "gulp-live-server": "~0.0",
        "gulp-open": "~3.0",
        "gulp-prettify": "~0.5",
        "gulp-rename": "~1.4",
        "gulp-sass": "~4.0",
        "gulp-uglify": "~3.0",
        "gulp-w3c-html-validator": "~1.4",
        "highlight.js": "~9.13",
        "js-yaml": "~3.12",
        "jshint": "~2.9",
        "marked": "~0.5",
        "merge-stream": "~1.0",
        "node-sass": "~4.9"
    }
}
